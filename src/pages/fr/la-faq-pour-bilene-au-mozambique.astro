---
import Menu from '../../components/Menu.astro';
import Footer from '../../components/Footer.astro';
import '../../sass/general.scss';
---

<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css2?family=Roboto&family=Shrikhand&display=swap" rel="stylesheet">
    <title>FAQ Bilene au Mozambique</title>
    <meta name="description" content="Les questions fréquement posées au sujet de Bilene Kite Center au Mozambique">
    <meta name="keywords" content="logement,logements, hotel, restaurant, beah-bar, bar, plage, resort, Palmeiras, San Martinho, Sunset, visas, vols, compagnies aériennes, kite, kitesurf, kiteboard, wing, wingsurf, SUP, paddle, Mozambique, Bilene, cours">
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-H1CJQ1EJQS"></script>
</head>
<body>
    <Menu />


    <main>
        <section id="headsection">
            <img src="/images/11-Accomodation/Palm_view.jpg" alt="Vue sur la lagune de Bilene">
            <h1 style="text-shadow: 1px 1px 2px black;">FAQ</h1>
            <h2 style="color: #b0cff7; text-shadow: 1px 1px 2px black;">Les questions posées fréquement</h2>
        </section>
        <section id="mainsection">
            <div class="flexdiv">
                <div class="flextext">
                    <h2>FAQ</h2>
                    <p>En cours de réalisation, consultez la <a href="/fr/infos-mozambique">page d'informations</a>
                    </p>
                </div>
                <div class="fleximg">
                    <img src="/images/inprogress.webp" alt="Photo d'un kitesurfeur dans les airs">
                </div>
            </div>
        </section>
    </main>
    <Footer />
</body>
</html>

---
import Menu from '../../components/Menu.astro';
import Footer from '../../components/Footer.astro';
import '../../css/general.css';
---

<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css2?family=Roboto&family=Shrikhand&display=swap" rel="stylesheet">
    <title>Sorties et restaurants à Bilene au Mozambique</title>
    <meta name="description" content="Les possibilités de logement que nous proposons à Bilene au Mozambique">
    <meta name="keywords" content="logement, logements, hotel, restaurant, beah-bar, bar, plage, resort, Palmeiras, San Martinho, Sunset">
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-H1CJQ1EJQS"></script>
</head>
<body>
    <Menu />


    <main>
        <section id="headsection">
            <img src="/images/11-Accomodation/Palm_view.jpg" alt="Vue sur la lagune de Bilene">
            <h1 style="text-shadow: 1px 1px 2px black;">Les sorties et restaurants à Bilene</h1>
            <h2 style="color: #b0cff7; text-shadow: 1px 1px 2px black;">Les bons moments en soirée</h2>
        </section>
        <section id="mainsection">
            <div class="flexdiv">
                <div class="flextext">
                    <h2>En cours de réalisation</h2>
                    <p>
                    </p>
                </div>
                <div class="fleximg">
                    <img src="/images/inprogress.webp" alt="photo d'un kitesurfer dans les airs">
                    <img src="/images/11-Accomodation/Palm_Bung.jpg" alt="bungalow dans la forêt">
                </div>
            </div>
        </section>
    </main>
    <Footer />
</body>
</html>
